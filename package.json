{
  "homepage": "https://bandejapadeltraining.com",
  "name": "Bandeja Padel Training",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "colorthief": "^2.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "gh-pages": "^2.2.0",
    "jest-canvas-mock": "^2.3.1",
    "lottie-react": "^2.4.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-easy-emoji": "^1.8.1",
    "react-headroom": "^3.0.0",
    "react-multi-carousel": "^2.8.5",
    "react-reveal": "^1.2.2",
    "react-router": "^5.3.4",
    "react-router-dom": "^6.28.2",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^16.10.2",
    "react-twitter-embed": "3.0.3",
    "sass": "^1.32.8"
  },
  "scripts": {
    "create-404": "cd build && cp index.html 404.html",
    "add-domain": "echo https://bandejapadeltraining.com > build/CNAME",
    "predeploy": "npm run build",
    "deploy": " npm run add-domain && npm run create-404 && gh-pages -d build",
    "start": "npm run build && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"./**/*.{js,css,json,scss}\"",
    "check-format": "prettier -c \"./**/*.{js,css,json,scss}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "dotenv": "^8.2.0",
    "prettier": "^2.3.1"
  }
}
